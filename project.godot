; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Jay's Reusable Components & Things"
config/description="A package of reusable components and random things for Godot."
config/version="1.0"
config/tags=PackedStringArray("plugin")
run/main_scene="res://addons/jaysreusablecomponentsandthings/example_scenes/levels/fps_example.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

DebugIt="*res://addons/jaysreusablecomponentsandthings/common_components/autoloads/debug/debugit.tscn"
DrawIt="*res://addons/jaysreusablecomponentsandthings/common_components/autoloads/draw/drawit.gd"
RaycastIt="*res://addons/jaysreusablecomponentsandthings/common_components/autoloads/raycast/raycastit.gd"
ResonateIt="*res://addons/jaysreusablecomponentsandthings/common_components/autoloads/audio/resonateit.gd"
SceneIt="*res://addons/jaysreusablecomponentsandthings/common_components/scene/scene_it.gd"
LevelIt="*res://addons/jaysreusablecomponentsandthings/common_components/scene/level/level_it.gd"

[debug]

settings/stdout/verbose_stdout=true
gdscript/warnings/exclude_addons=false
gdscript/warnings/untyped_declaration=2

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/always_on_top=true
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/gd_local_history/plugin.cfg", "res://addons/jaysreusablecomponentsandthings/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/jaysreusablecomponentsandthings/3d_components/": "teal",
"res://addons/jaysreusablecomponentsandthings/assets/": "green",
"res://addons/jaysreusablecomponentsandthings/common_components/": "gray",
"res://addons/jaysreusablecomponentsandthings/editor/": "red",
"res://addons/jaysreusablecomponentsandthings/example_scenes/": "purple",
"res://addons/jaysreusablecomponentsandthings/resources/": "blue"
}

[filesystem]

import/fbx2gltf/enabled=false

